{
  "name": "xls_parser",
  "version": "0.0.5",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "electron-is-dev": "^2.0.0",
    "exceljs": "^4.4.0",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "wait-port": "^1.1.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "concurrently \"yarn react-scripts start\" \"yarn electron\" ",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "react-scripts start",
    "electron": "wait-port http://:3000 && electron .",
    "electron-pack": "yarn build && electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "build": {
    "appId": "com.example.xlsParser",
    "productName": "XLS Parser",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "src/lib/*"
    ],
    "extraFiles": [
      "*.dll",
      "*.lib"
    ],
    "directories": {
      "output": "assets",
      "buildResources": "assets"
    },
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis",
      "icon": "/public/app_icon_512.ico"
    }
  },
  "homepage": "./",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^27.1.2",
    "electron-builder": "^24.9.1"
  }
}
